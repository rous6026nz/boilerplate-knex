{
  "name": "zaine-boilerplate-knex",
  "version": "1.0.0",
  "description": "A boilerplate set up repo to create a empty NodeJS, ExpressJS, KnexJS, Jest and Supertest project with file and folder structure and dependencies.",
  "main": "index.js",
  "repository": "https://github.com/rous6026nz/boilerplate-knex.git",
  "author": "Zaine Kingi <rous6026nz@gmail.com>",
  "license": "ISC",
  "scripts": {
    "start": "webpack && node server",
    "dev": "node server & webpack --watch",
    "test": "jest --watchAll"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "knex": "^0.15.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "jest": "^23.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "sqlite3": "^4.0.2",
    "supertest": "^3.1.0",
    "webpack": "^4.17.1",
    "webpack-dev-server": "^3.1.7"
  }
}
